<div>

        <lable>Title</lable>
        <br>
        <input type="text" name="title" value="<%=person.title%>">
</div>

<div>
<label>user</label>
<br>
  <select name="user">
  <% users.forEach(user => { %>
    <% if (user.id === person.user) { %>
      <option selected label="<%= user.name %>" value="<%= user.id %>">
    <% } else { %>
      <option label="<%= user.name %>" value="<%= user.id %>">
    <% } %>
  <% }) %>
  </select>
</div>

<div>
    <lable>Date of birth</lable>
    <br>
    <input type="date" name="Datebirth" 
    value="<%=person.Datebirth==null ? '' : 
    person.Datebirth.toISOString().split('T')[0]%>">
</div>

<div>
    <lable>ID</lable>
    <br>
    <input type="number" name="ID" value="<%=person.ID %>">
</div>




<div>
    <lable>File</lable>
    <br>
    <input type="file" name="cover" class="filepond" %>">
</div>



